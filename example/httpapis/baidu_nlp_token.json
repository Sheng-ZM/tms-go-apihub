{
  "id": "baidu_nlp_token",
  "url": "https://aip.baidubce.com/oauth/2.0/token",
  "private": "baidu_nlp_key",
  "method": "GET",
  "requestContentType": "json",
  "args": [
    {
      "in": "query",
      "name": "grant_type",
      "value": {
        "from": "private",
        "content": "xgranttype"
      }
    },
    {
      "in": "query",
      "name": "client_id",
      "value": {
        "from": "private",
        "content": "xappkey"
      }
    },
    {
      "in": "query",
      "name": "client_secret",
      "value": {
        "from": "private",
        "content": "xsecret"
      }
    }
  ]
}