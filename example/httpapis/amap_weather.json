{
  "id": "amap_weather",
  "url": "https://restapi.amap.com/v3/weather/weatherInfo",
  "method": "GET",
  "private": "amap_keys",
  "args": [
    {
      "in": "query",
      "name": "key",
      "value": {
        "from": "private",
        "content": "key1"
      }
    },
    {
      "in": "query",
      "name": "city",
      "value": {
        "from": "origin",
        "content": "city"
      }
    }
  ]
}
