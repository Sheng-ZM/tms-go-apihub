{
  "id": "kdxf_nlp_ke",
  "description": "科大讯飞自然语言处理，提取关键词。",
  "url": "https://ltpapi.xfyun.cn/v1/ke",
  "method": "POST",
  "private": "kdxf_keys",
  "requestContentType": "form",
  "args": [
    {
      "in": "header",
      "name": "X-Appid",
      "value": {
        "from": "private",
        "content": "appid"
      }
    },
    {
      "in": "header",
      "name": "X-CurTime",
      "value": {
        "from": "func",
        "content": "utc"
      }
    },
    {
      "in": "header",
      "name": "X-Param",
      "value": {
        "from": "literal",
        "content": "eyJ0eXBlIjoiZGVwZW5kZW50In0="
      }
    },
    {
      "in": "vars",
      "name": "apikey",
      "value": {
        "from": "private",
        "content": "apikey"
      }
    },
    {
      "in": "vars",
      "name": "XCurTime",
      "value": {
        "from": "json",
        "content": "{{index .vars \"X-CurTime\"}}"
      }
    },
    {
      "in": "vars",
      "name": "XParam",
      "value": {
        "from": "json",
        "content": "{{index .vars \"X-Param\"}}"
      }
    },
    {
      "in": "header",
      "name": "X-CheckSum",
      "value": {
        "from": "json",
        "content": "{{md5 .vars.apikey .vars.XCurTime .vars.XParam}}"
      }
    },
    {
      "in": "body",
      "name": "text",
      "value": {
        "from": "origin",
        "content": "content"
      }
    }
  ]
}
