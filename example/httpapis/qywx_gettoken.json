{
  "id": "qywx_gettoken",
  "url": "https://qyapi.weixin.qq.com/cgi-bin/gettoken",
  "method": "GET",
  "private": "qywx_keys",
  "parameters": [
    {
      "in": "query",
      "name": "corpid",
      "value": {
        "from": "private",
        "content": "corpid"
      }
    },
    {
      "in": "query",
      "name": "corpsecret",
      "value": {
        "from": "private",
        "content": "corpsecret"
      }
    }
  ],
  "cache": {
  "expire": {
	  "from": "template",
	  "content": "{{.result.expires_in}}"
    },
	"format": "second"
  }
}
