{
  "id": "sm_send",
  "private": "sm_keys",
  "dynamicUrl": {
    "from": "private",
    "content": "url"
  },
  "method": "POST",
  "requestContentType": "json",
  "args": [
    {
      "in": "body",
      "name": "body",
      "value": {
        "from": "json",
        "json": {
          "type": 1,
          "to": "{{.origin.number}}",
          "content": "{{.origin.msg}}【佳信客服】"
        }
      }
    }
  ]
}
