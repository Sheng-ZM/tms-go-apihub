{
  "id": "qywx_message_send",
  "url": "https://qyapi.weixin.qq.com/cgi-bin/message/send",
  "method": "POST",
  "requestContentType": "json",
  "parameters": [
    {
      "in": "query",
      "name": "access_token",
      "from": {
        "from": "origin",
        "name": "token"
      }

    },
    {
      "in": "body",
      "name": "body",
      "from": {
        "from": "JsonTemplate",
        "template": {
          "touser": "{{.origin.touser}}",
          "msgtype": "{{.origin.msgtype}}",
          "agentid": "{{.origin.agentid}}",
          "text": { "content": "{{.origin.content}}" }
        }
      }
    }
  ],
  "respStatus": {
    "from": {
  		"from": "template",
  		"name": "{{.result.errcode}}"
  	},
    "format": "number",
    "expected": "0"
  }
}
