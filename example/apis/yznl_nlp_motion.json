{
  "id": "yznl_nlp_motion",
  "url": "http://10.128.86.64:8000/serviceAgent/rest/hx/txt2/nlpemotion",
  "private": "yznl_nlp_motion_keys",
  "method": "POST",
  "requestContentType": "json",
  "parameters": [
    {
      "in": "header",
      "name": "X-APP-ID",
      "value": {
        "from": "private",
        "content": "xappid"
      }
    },
    {
      "in": "header",
      "name": "X-APP-KEY",
      "value": {
        "from": "private",
        "content": "xappkey"
      }
    },
    {
      "in": "vars",
      "name": "utc",
      "value": {
        "from": "func",
        "content": "utc"
      }
    },
    {
      "in": "body",
      "name": "body",
      "value": {
        "from": "json",
        "json": {
          "text": "{{.origin.text}}",
          "seqid": "{{.origin.seqid}}",
          "timestamp": "{{.vars.utc}}"
        }
      }
    }
  ]
}
