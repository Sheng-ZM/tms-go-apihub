{
  "id": "baidu_image_classify_token",
  "url": "https://aip.baidubce.com/oauth/2.0/token",
  "private": "baidu_image_classify_key",
  "method": "GET",
  "requestContentType": "json",
  "expiresSrc": "header",
  "parameters": [
     {
      "in": "query",
      "name": "grant_type",
      "value": {
        "from": "private",
        "content": "xgranttype"
      }
    },
    {
      "in": "query",
      "name": "client_id",
      "value": {
        "from": "private",
        "content": "xappkey"
      }
    },
	{
      "in": "query",
      "name": "client_secret",
      "value": {
        "from": "private",
        "content": "xsecret"
      }
    }

  ],
  "cache": {
	"expire": {
		"from": "json",
		"content": "{{.result.expires_in}}"
	},
	"format": "second"
  }
}
