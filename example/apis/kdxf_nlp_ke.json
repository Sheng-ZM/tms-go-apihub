{
  "id": "kdxf_nlp_ke",
  "description": "科大讯飞自然语言处理，提取关键词。",
  "url": "https://ltpapi.xfyun.cn/v1/ke",
  "method": "POST",
  "private": "kdxf_keys",
  "requestContentType": "form",
  "parameters": [
    {
      "in": "header",
      "name": "X-Appid",
      "from": {
        "from": "private",
        "name": "appid"
      }
    },
    {
      "in": "header",
      "name": "X-CurTime",
      "from": {
        "from": "func",
        "name": "utc"
      }
    },
    {
      "in": "header",
      "name": "X-Param",
      "value": "eyJ0eXBlIjoiZGVwZW5kZW50In0="
    },
    {
      "in": "vars",
      "name": "apikey",
      "from": {
        "from": "private",
        "name": "apikey"
      }
    },
    {
      "in": "header",
      "name": "X-CheckSum",
      "from": {
        "from": "func",
        "name": "checkSum",
        "args": "apikey X-CurTime X-Param"
      }
    },
    {
      "in": "body",
      "name": "text",
      "from": {
        "from": "origin",
        "name": "content"
      }
    }
  ]
}
