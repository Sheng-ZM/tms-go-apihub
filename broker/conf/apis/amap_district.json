{
  "id": "amap_district",
  "private": "common",
  "description": "高德地图查询城市区域编码",
  "url": "https://restapi.amap.com/v3/config/district",
  "method": "GET",
  "parameters": [
    {
      "in": "query",
      "name": "key",
      "from": {
        "in": "private",
        "name": "amap_key1"
      }
    },
    {
      "in": "query",
      "name": "subdistrict",
      "value": "0"
    },
    {
      "in": "query",
      "name": "keywords",
      "from": {
        "in": "query",
        "name": "city"
      }
    }
  ],
  "response": {
    "json": {
      "errCode": "{{.status}}",
      "data": {
        "adcode": "{{(index .districts 0).adcode}}",
        "name": "{{(index .districts 0).name}}",
        "center": "{{(index .districts 0).center}}"
      }
    }
  }
}
